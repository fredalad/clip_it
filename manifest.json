  "manifest_version": 3,
  "name": "Mariano's Coupon Button",
  "version": "1.0",
  "description": "Injects a button into Mariano's coupon page",
  "permissions": ["scripting"],
  "content_scripts": [
    {
      "matches": ["https://www.marianos.com/savings/cl/coupons/"],
      "js": ["content.js"]
    }
  ]
  
}
